{
  "name": "vue-vuex-apollo-graphql-mongodb-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate containing the classic Todo implementation using Vue, Vuex, Apollo GraphQL, and MongoDB",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm run serve",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "keywords": [
    "vue",
    "vuex",
    "apollo",
    "graphql",
    "mongoDB"
  ],
  "author": "William Connatser",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.6.9",
    "bcrypt": "^3.0.6",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.6.4",
    "now-env": "^3.2.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "nodemon": "^1.19.1"
  }
}
